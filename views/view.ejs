    <main>
        <div class="container">
            <div class="row">
                <div class="col">
                    <% data.forEach((item)=> { %>
                        <li>
                            <%= item.username %>
                        </li>
                        <% }); %>
                </div>
            </div>
        </div>
    </main>
    <footer>
        <div class="container">
            <div class="row">
                <div class="col">

                </div>
            </div>
        </div>
    </footer>
</body>

</html>